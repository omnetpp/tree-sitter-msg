{
  "name": "msg",
  "version": "0.0.1",
  "description": "Tree-sitter grammar for OMNeT++ MSG files",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "author": "Rudolf Hornig",
  "license": "MIT",
  "dependencies": {
    "node-gyp-build": "^4.8.0",
    "node-addon-api": "^7.1.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.6"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "tree-sitter": [
    {
      "scope": "source.omnetpp_msg",
      "injection-regex": "^omnetpp_msg$",
      "file-types": [
        "msg"
      ]
    }
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omnetpp/tree-sitter-msg.git"
  },
  "keywords": [
    "omnetpp",
    "msg"
  ],
  "bugs": {
    "url": "https://github.com/omnetpp/tree-sitter-msg/issues"
  },
  "homepage": "https://github.com/omnetpp/tree-sitter-msg#readme"
}
